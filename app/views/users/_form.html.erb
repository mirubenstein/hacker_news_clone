<%= render("layouts/errors", :object => @user) %>

<%= form_for(@user) do |f| %>
  <div class="form-group">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
   <div class="form-group">
    <%= f.label :password_digest %>
    <%= f.text_field :password_digest %>
  </div>
  <%= f.submit(class: "btn btn-primary") %>
<% end %>
